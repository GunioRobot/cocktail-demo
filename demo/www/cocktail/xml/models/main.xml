<model>


	<!--  INLINE  -->

	<inline id="inline-test" inject="main,home" locale="pt">
		<name>nybras</name>
		<gender>25</gender>
		<age>25</age>
	</inline>



	<!--  XML  -->

	<xml id="xml-test" inject="main,home" locale="pt" src="file.xml">
		<name>{..category.(@name==string)..instrument[2].@name}</name>
		<age>{..category.(@name==string)..instrument[2].@age}</age>
	</xml>



	<!--  AMF  -->

	<amf id="amf-test" inject="main,home" locale="pt" src="ServiceTest">
		<test_method params="#LOCALE_CURRENT#|ASC">
			<name>{string.0}</name>
			<age>{string.1}</age>
		</test_method>
	</amf>



	<!--  HTTP  -->

	<http id="http-test-a" inject="main,home" locale="pt" src="test.php">
		<name>{RAW}</name>
	</http>

	<http id="http-test-b" inject="main,home" locale="pt" src="test.php">
		<name>{RAW}</name>
	</http>


</model>