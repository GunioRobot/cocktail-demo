package cocktail.lib.view.ui {	import cocktail.lib.View;	import cocktail.lib.view.styles.renders.Render;	import cocktail.lib.view.ui.events.UIEvent;			/**	 * Base UI class.	 * @author nybras | nybras@codeine.it	 */	public class UI extends UIEvent 	{		private var _renders : Array;				public function before_init () : void		{			_renders = [];		}				public function uses ( render : Render ) : void		{			render.boot ( _sprite, style () );			_renders.push( render );		}			}}