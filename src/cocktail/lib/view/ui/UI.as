package cocktail.lib.view.ui {	import cocktail.lib.View;	import cocktail.lib.view.styles.renders.Render;	import cocktail.lib.view.ui.events.UIEvent;		/**	 * Base UI class.	 * @author nybras | nybras@codeine.it	 */	public class UI extends UIEvent 	{		/* ---------------------------------------------------------------------			VARS		--------------------------------------------------------------------- */				private var _renders : Array = [];				/**		 * Add a render to the renders array.		 * @param render	Render to be added.		 */		protected function _add_render ( render : Render ) : void		{			_renders.push( render );			render.boot ( _sprite, _style );		}			}}